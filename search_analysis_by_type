      <table>
        <search base="search_info">
          <query>|rex field=provenance mode=sed "s/UI:Dashboard:.*/Dashboard/"
|chart sum(run_time) as Total_runtime by sh_label provenance 
|eval Total_runtime=round(Total_runtime,0)
|addtotals</query>
        </search>
